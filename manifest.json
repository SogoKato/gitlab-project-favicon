{
    "manifest_version": 2,
    "name": "GitLab-Project-Favicon",
    "version": "1.0",
  
    "description": "Make GitLab favicon project logo to distinguish many gl tabs.",
  
    "icons": {
      "48": "icons/gitlab-project-favicon-48.png"
    },

    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["gitlab_project_icon.js"]
      }
    ],

    "options_ui": {
      "page": "options.html"
    },
    
    "permissions": ["storage"]
  }
  